# {
"xvkbd -xsendevent -text '{'"
    m:0x8 + c:16
    Alt + 7 

# [
"xvkbd -xsendevent -text '['"
    m:0x8 + c:17
    Alt + 8 

# ]
"xvkbd -xsendevent -text ']'"
    m:0x8 + c:18
    Alt + 9 

# }
"xvkbd -xsendevent -text '}'"
    m:0x8 + c:19
    Alt + 0 

# multimedia keys
# Increase volume
"pactl set-sink-volume @DEFAULT_SINK@ +4000"
   XF86AudioRaiseVolume

# Decrease volume
"pactl set-sink-volume @DEFAULT_SINK@ -4000"
   XF86AudioLowerVolume

# Mute volume
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
   XF86AudioMute

# Mute microphone
"pactl set-source-mute @DEFAULT_SOURCE@ toggle"
   XF86AudioMicMute

# Pause
"playerctl play-pause"
   XF86AudioPlay

# Next
"playerctl next"
   XF86AudioNext

# Previous
"playerctl previous"
   XF86AudioPrev



